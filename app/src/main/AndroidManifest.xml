<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daypon.ccswwf.dailycoupon">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="com.android.vending.BILLING" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="{$GEO_API_KEY}" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShopActivity" />
        <activity
            android:name=".CouponPopupActivity"
            android:theme="@style/BottomDialogTheme" />
        <activity android:name=".CouponCompleteActivity" />
        <activity android:name=".PrivatePolicyActivity" />
        <activity android:name=".MainActivity"/>
        <activity android:name=".MembershipApplyActivity" />
        <activity android:name=".TermsActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".AlertActivity" />
        <activity android:name=".BirthActivity" />
        <activity android:name=".GenderActivity" />
        <activity android:name=".MembershipApplyCompleteActivity" />
        <activity android:name=".RegisterCompleteActivity" />
        <activity android:name=".GooglePaymentCancelActivity" />
        <activity android:name=".FindIdActivity" />
        <activity android:name=".FindPasswordActivity" />
        <activity android:name=".FindIdCompleteActivity" />
        <activity android:name=".SetPasswordActivity"></activity>
    </application>

</manifest>